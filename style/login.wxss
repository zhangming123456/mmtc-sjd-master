.house {
  width: 180rpx;
  height: 180rpx;
}
.input-area {
  text-align: center;
  margin-bottom: 121rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.textInput {
  width: 100%;
  height: 90rpx;
  border-radius: 5rpx;
  border: solid 1px #999;
  padding-left: 30rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
  margin-bottom: 30rpx;
  text-align: left;
  display: inline-block;
}
.passworkBox {
  position: relative;
  width: 100%;
  border-radius: 5rpx;
  border: solid 1px #999;
  margin-bottom: 30rpx;
  box-sizing: border-box;
  padding: 0 30rpx;
  overflow: hidden;
}
.passworkBox .flexBox-item {
  position: relative;
  text-align: right;
  height: 90rpx;
  line-height: 90rpx;
}
.passworkBox label {
  position: relative;
  width: 100%;
  height: 100%;
}
.passworkBox label .passwordInput {
  padding-right: 30rpx;
  text-align: left;
  width: 100%;
  height: 90rpx;
  box-sizing: border-box;
}
.passworkBox label .passwordInput.azm-trn-100 {
  position: absolute;
  top: -100%;
  left: 0;
  transform: translate(-100%, -100%);
}
.passworkBox .azm-icon {
  font-size: 38rpx;
  color: #999;
}
.house-wrap {
  padding-top: 90rpx;
  padding-bottom: 79rpx;
}
button.btn {
  width: 670rpx;
  line-height: 90rpx;
  border-radius: 5rpx;
  display: inline-block;
  background-image: linear-gradient(90deg, #35abfe 0, #3d70f7 100%);
  color: #fff;
}
button.btn[plain] {
  border: none;
  color: #fff;
}
.lookPassword {
  width: 34rpx;
  height: 15rpx;
  padding-right: 40rpx;
}
.lookPassword2 {
  width: 37rpx;
  height: 22rpx;
  padding-right: 40rpx;
}
.forget {
  text-align: right;
  font-size: 32rpx;
  color: #424242;
}
