page {
  background-color: #f4f4f4;
}
.azm-address {
  width: 100%;
  height: 284rpx;
  padding: 24rpx 30rpx;
  border: 1px solid #e9eaec;
  box-sizing: border-box;
  border-radius: 5rpx;
  font-size: 30rpx;
}
button.azm-btn-submit {
  margin-top: 85rpx;
  height: 90rpx;
  line-height: 86rpx;
  border: none;
  background-image: linear-gradient(90deg, #35abfe 0, #3d70f7 100%);
  color: #fff;
  font-size: 32rpx;
  border-radius: 5rpx;
}
